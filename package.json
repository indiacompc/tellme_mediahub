{
  "name": "mediahub_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lintold": "eslint .",
    "lint": "eslint . --ignore-pattern \"/src/client/\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "all": "yarn install && yarn upgrade && yarn build"
  },
  "contributors": [
    {
      "name": "Pratik Yawalkar",
      "email": "pratik26yawalkar@gmail.com",
      "url": "http://github.com/pratikviper/"
    }
  ],
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "none",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "crlf",
    "tabWidth": 2,
    "useTabs": true,
    "bracketSameLine": false,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindAttributes": [
      "theme"
    ],
    "tailwindFunctions": [
      "twMerge",
      "createTheme",
      "cn"
    ]
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "schema-dts": "^1.1.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lint": "^0.8.19",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
